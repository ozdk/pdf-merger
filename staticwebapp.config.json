{
  "routes": [
    { "route": "/*", "headers": { "cache-control": "must-revalidate, max-age=86400" } }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*.{png,jpg,gif}", "/css/*", "/lib/*"]
  },
  "mimeTypes": {
    ".json": "text/json",
    ".pdf": "application/pdf"
  }
}